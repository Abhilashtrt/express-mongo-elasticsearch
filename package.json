{
  "name": "express",
  "version": "1.2.2",
  "description": "Express boilerplate for building RESTful APIs",
  "author": "Jenish Patel <jenish4096@gmail.com>",
  "main": "src/index.js",
  "private": false,
  "license": "MIT",
  "engines": {
    "node": "12"
  },
  "scripts": {
    "precommit": "npm run lint",
    "start": "nodemon ./src/index.js",
    "seed": "node ./src/api/models/seeders/employees.seeder.js",
    "docker:seed": "docker exec -d nodejs npm run seed",
    "lint": "eslint ./src/ --ignore-path .gitignore --ignore-pattern internals/scripts",
    "lint:fix": "eslint ./src/ --ignore-path .gitignore --ignore-pattern internals/scripts --fix",
    "lint:watch": "eslint ./src/ --ignore-path .gitignore --ignore-pattern internals/scripts --watch",
    "postpublish": "git push --tags",
    "deploy": "sh ./deploy.sh",
    "postdocs": "opn docs/index.html",
    "docker:build:dev": "docker-compose -f docker-compose.dev.yml build",
    "docker:build:prod": "docker-compose -f docker-compose.prod.yml build",
    "docker:build:test": "docker-compose -f docker-compose.test.yml build",
    "docker:start:dev": "docker-compose -f docker-compose.dev.yml up -d",
    "docker:start:prod": "docker-compose -f docker-compose.prod.yml up -d",
    "docker:start:test": "docker-compose -f docker-compose.test.yml up -d",
    "docker:stop:dev": "docker-compose -f docker-compose.dev.yml down",
    "docker:stop:prod": "docker-compose -f docker-compose.prod.yml down",
    "docker:stop:test": "docker-compose -f docker-compose.test.yml down"
  },
  "keywords": [
    "express",
    "node",
    "node.js",
    "mongodb",
    "mongoose",
    "passport",
    "es6",
    "es7",
    "es8",
    "es2017",
    "mocha",
    "istanbul",
    "nyc",
    "eslint",
    "Travis CI",
    "coveralls",
    "REST",
    "API",
    "boilerplate",
    "generator",
    "starter project"
  ],
  "dependencies": {
    "axios": "0.18.0",
    "bcryptjs": "2.4.3",
    "bluebird": "3.5.0",
    "body-parser": "1.17.0",
    "compression": "1.6.2",
    "cors": "2.8.3",
    "cross-env": "5.0.1",
    "dotenv": "^8.2.0",
    "dotenv-safe": "6.0.0",
    "elasticsearch": "^16.4.0",
    "express": "4.15.2",
    "express-validation": "1.0.2",
    "helmet": "3.5.0",
    "http-status": "1.0.1",
    "joi": "10.4.1",
    "jwt-simple": "0.5.2",
    "lodash": "^4.17.11",
    "method-override": "3.0.0",
    "moment-timezone": "0.5.13",
    "mongoose": "5.2.17",
    "morgan": "1.9.1",
    "passport": "0.4.0",
    "passport-http-bearer": "1.0.1",
    "passport-jwt": "4.0.0",
    "pm2": "3.0.0",
    "uuid": "3.1.0",
    "winston": "3.1.0"
  },
  "devDependencies": {
    "apidoc": "0.17.5",
    "chai": "4.1.0",
    "chai-as-promised": "7.1.1",
    "coveralls": "3.0.0",
    "eslint": "5.3.0",
    "eslint-config-airbnb-base": "12.0.1",
    "eslint-plugin-import": "2.2.0",
    "husky": "0.14.3",
    "mocha": "3.3.0",
    "nodemon": "1.11.0",
    "nyc": "12.0.2",
    "opn-cli": "3.1.0",
    "sinon": "6.0.0",
    "sinon-chai": "3.0.0",
    "supertest": "3.0.0"
  }
}
